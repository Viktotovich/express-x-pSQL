<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title%></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% usernames.forEach((user) => { %>
      <li>
        <p><%= user.username%></p>
      </li>
      <%})%>
    </ul>
    <a href="/new">New Username</a>
    <%- include("../partials/search")%>
    <h2>Delete all users</h2>
    <form action="/delete" method="GET">
      <button type="submit" onclick="return confirm('Delete all users?')">
        Delete Everyone
      </button>
    </form>
  </body>
</html>
